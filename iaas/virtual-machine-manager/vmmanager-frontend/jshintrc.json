{
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "globals": {
        "jQuery": true,
        "angular": true,
        "toastr": true,
        "breeze": true,
        "moment": true,
        "q": true
    },
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": false,
    "unused": false,
    "strict": false,
    "maxparams": 10,
    "maxdepth": 4,
    "trailing": true,
    "maxlen": 120,
    "browser": true,
    "node": true,
    "expr": false,
    "validthis": true
}