#!/bin/bash

echo "UPDATING APPs IN NODES"

scp -rv /home/cdiaz/workspaceS/severo1.3/apps/ bscgrid05:/home/cdiaz/workspaceS/severo1.3/
scp -rv /home/cdiaz/workspaceS/severo1.3/apps/ bscgrid06:/home/cdiaz/workspaceS/severo1.3/

echo "DONE!"