      <nodes xsi:type="grl:Belief" name="Belief6034" id="6034" description="Determining worthiness to refactor can take place through uncontrolled or controlled usage experiments" x="1711" y="-195" succ="//@urndef/@specDiagrams.4/@connections.29" author="jcd">
        <metadata name="_width" value="242"/>
